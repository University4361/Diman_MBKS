<Window x:Class="Pick_lab2.RemoveAccessWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pick_lab2"
        mc:Ignorable="d"
        Title="RemoveAccessWindow" Height="300" Width="850">
    <Grid>
        <DockPanel x:Name="RulesDP" LastChildFill="True" Margin="10,0,90,10" Height="40" VerticalAlignment="Bottom"/>
        <Button Content="Remove" Margin="0,0,10,10" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" Click="RemoveButton_Click"/>
        <StackPanel Name="MainSP" Margin="10,10,10,55"/>

    </Grid>
</Window>
